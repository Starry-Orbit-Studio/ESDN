{
  "name": "extreme-starry-docs",
  "type": "module",
  "description": "Extreme Starry 文档站",
  "license": "CC-BY-NC-SA 4.0",
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677",
  "scripts": {
    "build": "vuepress build docs",
    "clean-dev": "vuepress dev docs --clean-cache",
    "dev": "vuepress dev docs",
    "format": "prettier --write .",
    "postinstall": "husky",
    "update-package": "pnpm dlx vp-update"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.22",
    "@vuepress/plugin-feed": "2.0.0-rc.99",
    "@vuepress/plugin-slimsearch": "2.0.0-rc.99",
    "htmlparser2": "^10.0.0",
    "husky": "^9.1.7",
    "mermaid": "^11.6.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-pkg": "^0.19.0",
    "sass-embedded": "^1.87.0",
    "typescript": "^5.8.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10",
    "vuepress": "2.0.0-rc.22",
    "vuepress-theme-hope": "2.0.0-rc.85"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-pkg"
    ]
  }
}
